                    </div>
                </div>
            </div>
        </div>
    </div>
    <br/>
</div>
<script>console.log("Welcome to Blocky"); console.log("Blocking domains the way you want it")</script>
</body>
<script>

    @{
        long epocSeconds = 1488438130;
        var dateTimeOffset = DateTimeOffset.FromUnixTimeSeconds(epocSeconds);
        var creationDate = dateTimeOffset.DateTime;
    }

        window.onload = function () {
            var creationDate = new Date('@creationDate.ToString("yyyy-MM-ddTHH:mm:ss")');
            elapsedTime(creationDate);
        }

    var date;
    Date.prototype.getElapsedTime = function () {
        var diffDate = new Date(new Date() - this);
        return "Blocky was first launched: " + (diffDate.getFullYear() - 1970) + " Years, " + diffDate.getMonth() + " Months, " + (diffDate.getDate() - 1) + " Days, " + diffDate.getHours() + " Hours, " + diffDate.getMinutes() + " Minutes and " + diffDate.getSeconds() + " Seconds ago.";
    };

    function elapsedTime(givenDate) {
        var result = givenDate.getElapsedTime();
        document.getElementById('launchTimer').innerHTML = result;
        clearTimeout(elapsedTime.to);
        elapsedTime.to = setTimeout(function () {
            elapsedTime(givenDate);
        }, 1000);
    }
</script>