@page
@using EonaCat.Blocky
@using EonaCat.Dns.Managers

<style>
    .total {
        background-color: @StatsManagerApi.TotalQueriesBackgroundColor;
        color: #ffffff;
    }

    .blocked {
        background-color: @StatsManagerApi.BlockedBackgroundColor;
        color: #ffffff;
    }

    .cached {
        background-color: @StatsManagerApi.CachedBackgroundColor;
        color: #ffffff;
    }

    .clients {
        background-color: @StatsManagerApi.ClientsBackgroundColor;
        color: #ffffff;
    }
</style>

<script src="/js/signalr/dist/browser/signalr.min.js"></script>
<script>
    $(function () {
        // Connect to the SignalR hub
        var connection = new signalR.HubConnectionBuilder().withUrl("/blockyHub").build();

        // Retrieve the new data
        async function startDataRetrieval() {
            await connection.start();
            connection.on("updateData", (data) => {
                // Update the UI with the new data
                $("#total-queries").text(data.totalQueries.toString().replace(/\B(?=(\d{3})+(?!\d))/g, "."));
                $("#total-cached").text(data.totalCached.toString().replace(/\B(?=(\d{3})+(?!\d))/g, "."));
                $("#total-blocked").text(data.totalBlocked.toString().replace(/\B(?=(\d{3})+(?!\d))/g, "."));
                $("#total-clients").text(data.totalClients.toString().replace(/\B(?=(\d{3})+(?!\d))/g, "."));
                $("#total-allowList").text(data.totalAllowList.toString().replace(/\B(?=(\d{3})+(?!\d))/g, "."));
                $("#total-blockList").text(data.totalBlockList.toString().replace(/\B(?=(\d{3})+(?!\d))/g, "."));
            });
        }

        startDataRetrieval();
    });
</script>

@{
    var dnsStats = await BlockyDns.GetDnsStatsAsync().ConfigureAwait(false);
}

<body style="background-color: black; color: white;">
<div class="header">
    <div id="launchTimer"></div>
